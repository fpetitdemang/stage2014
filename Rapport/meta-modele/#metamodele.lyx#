\begin_layout Plain Layout

	basicstyle=
\backslash
footnotesize, 
\end_layout

\begin_layout Plain Layout

	frame=tb,   
\end_layout

\begin_layout Plain Layout

	xleftmargin=.2
\backslash
textwidth, 
\end_layout

\begin_layout Plain Layout

	xrightmargin=.2
\backslash
textwidth 
\end_layout

\begin_layout Plain Layout

} 
\end_layout

