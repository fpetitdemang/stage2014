package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Connector;
import uml.Interaction;
import uml.MessageEnd;
import uml.MessageKind;
import uml.MessageSort;
import uml.NamedElement;
import uml.ValueSpecification;

/**
 * A sample validator interface for {@link uml.Message}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface MessageValidator {
	boolean validate();

	boolean validateMessageKind(MessageKind value);
	boolean validateMessageSort(MessageSort value);
	boolean validateReceiveEvent(MessageEnd value);
	boolean validateSendEvent(MessageEnd value);
	boolean validateConnector(Connector value);
	boolean validateInteraction(Interaction value);
	boolean validateArgument(EList<ValueSpecification> value);
	boolean validateSignature(NamedElement value);
}
