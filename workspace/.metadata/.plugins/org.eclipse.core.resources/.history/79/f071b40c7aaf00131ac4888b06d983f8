package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.PackageMerge;
import uml.PackageableElement;
import uml.ProfileApplication;
import uml.Type;

/**
 * A sample validator interface for {@link uml.Package}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface PackageValidator {
	boolean validate();

	boolean validateOwnedType(EList<Type> value);
	boolean validatePackageMerge(EList<PackageMerge> value);
	boolean validatePackagedElement(EList<PackageableElement> value);
	boolean validateNestedPackage(EList<uml.Package> value);
	boolean validateNestingPackage(uml.Package value);
	boolean validateProfileApplication(EList<ProfileApplication> value);
}
