package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Dependency;
import uml.Namespace;
import uml.StringExpression;
import uml.VisibilityKind;

/**
 * A sample validator interface for {@link uml.NamedElement}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface NamedElementValidator {
	boolean validate();

	boolean validateName(String value);
	boolean validateVisibility(VisibilityKind value);
	boolean validateQualifiedName(String value);
	boolean validateClientDependency(EList<Dependency> value);
	boolean validateNamespace(Namespace value);
	boolean validateNameExpression(StringExpression value);
}
