package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Classifier;
import uml.Generalization;

/**
 * A sample validator interface for {@link uml.GeneralizationSet}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface GeneralizationSetValidator {
	boolean validate();

	boolean validateIsCovering(boolean value);
	boolean validateIsDisjoint(boolean value);
	boolean validatePowertype(Classifier value);
	boolean validateGeneralization(EList<Generalization> value);
}
