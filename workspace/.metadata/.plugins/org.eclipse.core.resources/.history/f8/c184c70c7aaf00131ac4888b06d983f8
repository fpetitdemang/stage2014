package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Constraint;
import uml.ElementImport;
import uml.NamedElement;
import uml.PackageImport;
import uml.PackageableElement;

/**
 * A sample validator interface for {@link uml.Namespace}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface NamespaceValidator {
	boolean validate();

	boolean validateElementImport(EList<ElementImport> value);
	boolean validatePackageImport(EList<PackageImport> value);
	boolean validateOwnedRule(EList<Constraint> value);
	boolean validateMember(EList<NamedElement> value);
	boolean validateImportedMember(EList<PackageableElement> value);
	boolean validateOwnedMember(EList<NamedElement> value);
}
