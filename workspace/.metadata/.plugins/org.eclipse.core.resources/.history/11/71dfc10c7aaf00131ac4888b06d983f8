package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.ActivityEdge;
import uml.ActivityNode;
import uml.ActivityPartition;
import uml.Element;

/**
 * A sample validator interface for {@link uml.ActivityPartition}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface ActivityPartitionValidator {
	boolean validate();

	boolean validateIsDimension(boolean value);
	boolean validateIsExternal(boolean value);
	boolean validateNode(EList<ActivityNode> value);
	boolean validateSubpartition(EList<ActivityPartition> value);
	boolean validateSuperPartition(ActivityPartition value);
	boolean validateRepresents(Element value);
	boolean validateEdge(EList<ActivityEdge> value);
}
