package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.ExecutableNode;
import uml.InputPin;
import uml.OutputPin;

/**
 * A sample validator interface for {@link uml.LoopNode}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface LoopNodeValidator {
	boolean validate();

	boolean validateIsTestedFirst(boolean value);
	boolean validateBodyPart(EList<ExecutableNode> value);
	boolean validateSetupPart(EList<ExecutableNode> value);
	boolean validateDecider(OutputPin value);
	boolean validateTest(EList<ExecutableNode> value);
	boolean validateResult(EList<OutputPin> value);
	boolean validateLoopVariable(EList<OutputPin> value);
	boolean validateBodyOutput(EList<OutputPin> value);
	boolean validateLoopVariableInput(EList<InputPin> value);
}
