package uml.validation;

import uml.ParameterableElement;
import uml.TemplateSignature;

/**
 * A sample validator interface for {@link uml.TemplateParameter}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface TemplateParameterValidator {
	boolean validate();

	boolean validateSignature(TemplateSignature value);
	boolean validateParameteredElement(ParameterableElement value);
	boolean validateOwnedParameteredElement(ParameterableElement value);
	boolean validateDefault(ParameterableElement value);
	boolean validateOwnedDefault(ParameterableElement value);
}
