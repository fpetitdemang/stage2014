package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Behavior;
import uml.CallConcurrencyKind;
import uml.Parameter;
import uml.ParameterSet;
import uml.Type;

/**
 * A sample validator interface for {@link uml.BehavioralFeature}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface BehavioralFeatureValidator {
	boolean validate();

	boolean validateOwnedParameter(EList<Parameter> value);
	boolean validateIsAbstract(boolean value);
	boolean validateMethod(EList<Behavior> value);
	boolean validateConcurrency(CallConcurrencyKind value);
	boolean validateRaisedException(EList<Type> value);
	boolean validateOwnedParameterSet(EList<ParameterSet> value);
}
