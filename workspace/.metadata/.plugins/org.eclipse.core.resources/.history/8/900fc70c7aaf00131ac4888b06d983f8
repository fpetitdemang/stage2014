package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Behavior;
import uml.BehavioralFeature;
import uml.BehavioredClassifier;
import uml.Constraint;
import uml.Parameter;
import uml.ParameterSet;

/**
 * A sample validator interface for {@link uml.Behavior}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface BehaviorValidator {
	boolean validate();

	boolean validateIsReentrant(boolean value);
	boolean validateRedefinedBehavior(EList<Behavior> value);
	boolean validateOwnedParameter(EList<Parameter> value);
	boolean validateContext(BehavioredClassifier value);
	boolean validatePrecondition(EList<Constraint> value);
	boolean validatePostcondition(EList<Constraint> value);
	boolean validateOwnedParameterSet(EList<ParameterSet> value);
	boolean validateSpecification(BehavioralFeature value);
}
