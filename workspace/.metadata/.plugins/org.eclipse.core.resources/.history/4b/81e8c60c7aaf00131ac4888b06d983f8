package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Association;
import uml.Behavior;
import uml.Connector;
import uml.ConnectorEnd;
import uml.ConnectorKind;

/**
 * A sample validator interface for {@link uml.Connector}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface ConnectorValidator {
	boolean validate();

	boolean validateType(Association value);
	boolean validateRedefinedConnector(EList<Connector> value);
	boolean validateEnd(EList<ConnectorEnd> value);
	boolean validateKind(ConnectorKind value);
	boolean validateContract(EList<Behavior> value);
}
