package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.ParameterableElement;
import uml.TemplateBinding;
import uml.TemplateParameter;

/**
 * A sample validator interface for {@link uml.TemplateParameterSubstitution}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface TemplateParameterSubstitutionValidator {
	boolean validate();

	boolean validateFormal(TemplateParameter value);
	boolean validateActual(EList<ParameterableElement> value);
	boolean validateOwnedActual(EList<ParameterableElement> value);
	boolean validateTemplateBinding(TemplateBinding value);
}
