package uml.validation;

import org.eclipse.emf.common.util.EList;

import uml.Region;
import uml.State;
import uml.StateMachine;
import uml.Transition;
import uml.Vertex;

/**
 * A sample validator interface for {@link uml.Region}.
 * This doesn't really do anything, and it's not a real EMF artifact.
 * It was generated by the org.eclipse.emf.examples.generator.validator plug-in to illustrate how EMF's code generator can be extended.
 * This can be disabled with -vmargs -Dorg.eclipse.emf.examples.generator.validator=false.
 */
public interface RegionValidator {
	boolean validate();

	boolean validateSubvertex(EList<Vertex> value);
	boolean validateTransition(EList<Transition> value);
	boolean validateState(State value);
	boolean validateExtendedRegion(Region value);
	boolean validateStateMachine(StateMachine value);
}
